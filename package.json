{
  "name": "ail-frontend",
  "version": "0.1.0",
  "private": true,
  "resolutions": {
    "styled-components": "^5"
  },
  "pre-commit": [
    "check:ts",
    "lint:js",
    "lint:css"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "env-cmd -e dev react-scripts start",
    "start-local": "env-cmd -e local react-scripts start",
    "check:ts": "tsc --noEmit --project tsconfig.json",
    "fix-lint:js": "eslint --fix -c ./.eslintrc.js --ext .js,.jsx,.ts,.tsx src",
    "lint:css": "stylelint '**/*.{css,js,ts}'",
    "lint:js": "eslint -c ./.eslintrc.js --ext .js,.jsx,.ts,.tsx src",
    "build": "env-cmd -e prod react-scripts build",
    "build-dev": "env-cmd -e dev react-scripts build",
    "build-prod": "env-cmd -e prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && tslint -c tslint.json -p tsconfig.json '**/*.ts{,x}' -e '**/node_modules/**'"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.272.0",
    "@aws-sdk/client-s3": "3.272.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.272.0",
    "@aws-sdk/lib-storage": "3.272.0",
    "@aws-sdk/s3-request-presigner": "3.272.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookform/error-message": "^2.0.0",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.5",
    "@mui/styled-engine-sc": "^5.8.0",
    "@mui/x-data-grid": "5.17.22",
    "@popperjs/core": "^2.11.5",
    "@reduxjs/toolkit": "^1.8.2",
    "@sentry/react": "^7.20.1",
    "@sentry/tracing": "^7.20.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@tranqi/ui-kit": "1.4.6",
    "amazon-cognito-identity-js": "^6.1.2",
    "await-to-js": "3.0.0",
    "aws-s3": "^2.0.5",
    "aws-sdk": "^2.1313.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "buffer": "^6.0.3",
    "env-cmd": "^10.1.0",
    "jwt-decode": "^3.1.2",
    "lz-string": "^1.4.4",
    "moment": "^2.29.3",
    "moment-business": "^3.0.1",
    "react": "^18.2.0",
    "react-animations": "^1.0.0",
    "react-cookie": "4.1.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-dropzone-uploader": "^2.11.0",
    "react-hook-form": "^7.33.0",
    "react-images-uploading": "^3.1.6",
